<% provide(:title, @company) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      Company Name: <%= @company.name %>
      Company Description: <%= @company.description %>
      Company Website: <%= @company.website %>

      <% @reviews.each do |review| %>
        <%= review.inspect %>
      <% end %>
    </section>
  </aside>
</div>
